        "eslint-import-resolver-webpack": "0.11.1",
        "eslint-loader": "2.2.1",
        "eslint-plugin-html": "6.0.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-jest": "24.0.2",
        "eslint-plugin-promise": "4.2.1",
        "inject-loader": "4.0.1",
        "sass-loader": "7.1.0",
        "stylelint": "10.1.0",
